{
  "name": "webdriverio-boilerplate",
  "version": "3.0.0",
  "description": "boilerplate framework for testing using node and webdriverio",
  "main": "wdio.conf.js",
  "engines": {
    "node": ">= 12.16.1"
  },
  "scripts": {
    "test": "node_modules/.bin/wdio examples/Amazon/wdio.conf.js",
    "test:e2e": "node_modules/.bin/wdio examples/Amazon/wdio.conf.js --suite e2e",
    "test:integration": "node_modules/.bin/wdio examples/Amazon/wdio.conf.js --suite integration",
    "test:local": "node_modules/.bin/wdio examples/Amazon/wdio.conf.local.js --suite dev"
  },
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.20.6",
    "@wdio/browserstack-service": "^8.0.6",
    "@wdio/cli": "^8.0.6",
    "@wdio/devtools-service": "^8.0.6",
    "@wdio/dot-reporter": "^8.0.6",
    "@wdio/local-runner": "^8.0.6",
    "@wdio/mocha-framework": "^8.0.6",
    "@wdio/selenium-standalone-service": "^8.0.6",
    "@wdio/spec-reporter": "^8.0.6",
    "@wdio/sync": "^7.27.0",
    "chai": "^4.3.7",
    "cheerio": "^1.0.0-rc.12",
    "chromedriver": "^108.0.0",
    "core-js": "^3.26.1",
    "dotenv": "^16.0.3",
    "mocha": "^10.1.0",
    "selenium-standalone": "^8.2.4",
    "wdio-chromedriver-service": "^8.0.1",
    "webdriverio": "^8.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonyet/webdriverio-boilerplate.git"
  },
  "author": "jonathan terry",
  "license": "MIT"
}
